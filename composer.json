{
    "version": "1.1.2",
    "name": "sendpost/sendpost_php_sdk",
    "description": "Email API and SMTP relay to not just send and measure email sending, but also alert and optimise. We provide you with tools, expertise and support needed to reliably deliver emails to your customers inboxes on time, every time.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/sendpost/sendpost_php_sdk",
    "license": "unlicense",
    "authors": [
        {
            "name": "Sendpost",
            "homepage": "https://github.com/sendpost/sendpost_php_sdk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "sendpost\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "sendpost\\Test\\" : "test/" }
    }
}
